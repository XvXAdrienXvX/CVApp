
@foreach (var skills in Model.skills)
{
    <div class="skills">
        <svg viewBox="0 0 36 36" class="circular-chart">
            <path class="circle-bg"
                  d="M18 2.0845
                              a 15.9155 15.9155 0 0 1 0 31.831
                              a 15.9155 15.9155 0 0 1 0 -31.831" />
            <path class="circle"
                  stroke-dasharray="@skills.SkillLevel, 100"
                  d="M18 2.0845
                              a 15.9155 15.9155 0 0 1 0 31.831
                              a 15.9155 15.9155 0 0 1 0 -31.831" />
            <text x="50%" y="50%" font-size="0.3em" text-anchor="middle" stroke="black" stroke-width="0.05px" dy=".3em">@skills.ShortName @skills.SkillLevel %</text>
        </svg>

    </div>
}